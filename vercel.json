{
  "version": 2,
  "builds": [
    { "src": "src/index.js", "use": "@vercel/node" },
    { "src": "src/api/generate.js", "use": "@vercel/node" },
    { "src": "src/public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/generate", "dest": "/src/api/generate.js" },
    { "handle": "filesystem" },
    { "src": "/", "dest": "/src/public/index.html" },
    { "src": "/(.+)", "dest": "/src/public/$1" },
    { "src": "/(.*)", "dest": "/src/index.js" }
  ]
}